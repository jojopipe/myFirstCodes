#include <iostream>
#include <cstdlib>

void bubbleSort(int *nums) {
    for (int i = 0; i < 10; i++)
    {
        for (int j = 0; j < 9; j++)
        {
            if (nums[j] >= nums[j+1])
            {
                int swap = nums[j];
                nums[j] = nums[j+1];
                nums[j + 1] = swap;
            }
        }
    }
}

int main()
{
    int nums[10];
    for (int i = 0; i < 10; i++) {
        nums[i] = rand() % 100;
    }

    for (int i = 0; i < 10; i++) {
        std::cout << nums[i] << "\n";
    }

    bubbleSort(nums);

    std::cout << "----------\n";

    for (int i = 0; i < 10; i++) {
        std::cout << nums[i] << "\n";
    }
}
